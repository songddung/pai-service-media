generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Media {
  media_id      BigInt @id @default(autoincrement())
  file_name     String
  mime_type     String
  file_size     BigInt
  s3_key        String
  cdn_url       String
  created_at    DateTime @default(now())
}